{
    "name": "burlang/site",
    "description": "Russian-Buryat, Buryat-Russian electronic dictionary",
    "keywords": ["buryat dictionary", "burlang", "buryat-lang"],
    "homepage": "https://burlang.ru/",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "bower-asset/bililiterange": "*",
        "bower-asset/codemirror": "~5.9.0",
        "bower-asset/codemirror-buttons": "~1.0.5",
        "mobiledetect/mobiledetectlib": "~2.8.45",
        "npm-asset/htmx.org": "^1.9.12",
        "yiisoft/yii2": "~2.0.49",
        "yiisoft/yii2-bootstrap": "~2.0.11",
        "yiisoft/yii2-symfonymailer": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
